# Local MongoDB connection for development
QUARKUS_MONGODB_CONNECTION_STRING=mongodb://mongodb:27017/primos-db
# Frontend uses this value to call the backend APIs. When running locally the
# browser cannot resolve the container name "backend" so use localhost instead.
BACKEND_URL=http://localhost:8080
CORS_ORIGINS=http://localhost:3000
REACT_APP_BACKEND_URL=http://localhost:8080
REACT_APP_PRIMOS_COLLECTION=primos
# For local development, use direct Magic Eden API (CORS works with localhost)
# REACT_APP_MAGICEDEN_BASE=/api/proxy
